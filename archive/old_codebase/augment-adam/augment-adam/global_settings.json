{
  "model": {
    "default_model": "llama3:8b",
    "temperature": 0.7,
    "max_tokens": 1024,
    "top_p": 0.9,
    "top_k": 40,
    "stop_sequences": [],
    "repetition_penalty": 1.1
  },
  "memory": {
    "working_memory_size": 10,
    "semantic_memory_enabled": true,
    "episodic_memory_enabled": true,
    "max_episodic_memories": 100,
    "default_memory_backend": "faiss"
  },
  "ui": {
    "theme": "light",
    "font_size": 14,
    "show_timestamps": true,
    "show_typing_animation": true,
    "code_highlighting": true
  },
  "plugins": {
    "enabled_plugins": [],
    "auto_enable_plugins": false,
    "plugin_timeout": 30.0
  },
  "logging": {
    "level": "INFO",
    "file_logging_enabled": true,
    "log_file_path": null,
    "log_format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "max_log_file_size": 10485760,
    "backup_count": 3
  },
  "security": {
    "enable_authentication": false,
    "session_timeout": 3600,
    "allowed_hosts": [
      "localhost",
      "127.0.0.1"
    ],
    "enable_cors": false,
    "cors_origins": []
  },
  "network": {
    "request_timeout": 30.0,
    "max_retries": 3,
    "retry_delay": 1.0,
    "proxy_enabled": false,
    "proxy_url": null
  },
  "custom": {}
}